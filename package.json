{
  "name": "a11y-web",
  "version": "0.2.3",
  "description": "a11y WIP",
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack-cli --watch --config webpack.config.js",
    "build": "webpack --mode=production",
    "test": "eslint . --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiessenp/a11y-web.git"
  },
  "author": "thiessenp",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/thiessenp/a11y-web/issues"
  },
  "homepage": "https://github.com/thiessenp/a11y-web#readme",
  "dependencies": {
    "keynav-web": "^1.1.7",
    "lodash": "^4.17.20"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.1",
    "eslint": "^7.19.0",
    "eslint-config-google": "^0.14.0",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "@babel/plugin-proposal-private-methods",
        {
          "loose": true
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "google"
    ],
    "env": {
      "node": true,
      "browser": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "rules": {
      "max-len": [
        2,
        120,
        4,
        {
          "ignoreUrls": true
        }
      ],
      "no-console": 0,
      "no-invalid-this": 0
    }
  },
  "eslintIgnore": [
    "node_modules",
    "dist",
    "test"
  ],
  "files": [
    "dist",
    "lib"
  ]
}
